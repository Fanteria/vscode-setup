{
  "workbench.colorTheme": "Default Dark+",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.lineNumbers": "relative",

  "window.menuBarVisibility": "classic",

  "editor.cursorSurroundingLines": 10,
  "editor.scrollbar.horizontal": "visible",
  "editor.scrollbar.vertical": "visible",
  "editor.scrollbar.verticalScrollbarSize": 30,

  "editor.hideCursorInOverviewRuler": false,

  "git.confirmSync": false,

  "editor.minimap.enabled": true,
  "editor.inlayHints.enabled": true,

  "workbench.activityBar.visible": true,
  "editor.formatOnSave": true,

  "markdown-preview-enhanced.previewTheme": "atom-dark.css",
  "markdown-preview-enhanced.enableScriptExecution": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,

  "vim.useSystemClipboard": true,
  "vim.foldfix": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["a", "<enter>", "<esc>"]
    },
    {
      "before": ["<leader>", "O"],
      "after": ["l", "v", "$", "h", "x", "O", "<esc>", "P"]
    },

    {
      "before": ["<leader>", "v", "l"],
      "commands": ["workbench.action.moveEditorToRightGroup"]
    },
    {
      "before": ["<leader>", "v", "k"],
      "commands": ["workbench.action.moveEditorToAboveGroup"]
    },
    {
      "before": ["<leader>", "v", "j"],
      "commands": ["workbench.action.moveEditorToBelowGroup"]
    },
    {
      "before": ["<leader>", "v", "h"],
      "commands": ["workbench.action.moveEditorToLeftGroup"]
    },

    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.action.gotoSymbol"]
    },
  ],
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-b>": false,
    "<C-k>": false,
    "<C-/>": false,
  },
  "clang.cxxflags": [
    "[\"-std=c++17\"]"
  ],
  "cSpell.language": "en,cs",
  "cSpell.userWords": [
    "GPIB",
    "promenna"
  ],
  "explorer.confirmDelete": false,
  "terminal.integrated.fontFamily": "monospace",

